{
  "compilerOptions": {
    "outDir": "./build",
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es6",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
